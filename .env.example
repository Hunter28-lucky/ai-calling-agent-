# LiveKit Credentials (https://cloud.livekit.io/)
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# Deepgram (STT & TTS)
# Sign up at https://console.deepgram.com/
DEEPGRAM_API_KEY=your_deepgram_api_key

# Groq (LLM)
# Sign up at https://console.groq.com/
GROQ_API_KEY=your_groq_api_key
LLM_PROVIDER=groq
# Default Model: llama-3.3-70b-versatile
GROQ_MODEL=llama-3.3-70b-versatile

# SIP Trunking Configuration
VOBIZ_SIP_TRUNK_ID=your_sip_trunk_id
OUTBOUND_TRUNK_ID=your_outbound_trunk_id
VOBIZ_SIP_DOMAIN=your_sip_domain
VOBIZ_USERNAME=your_sip_username
VOBIZ_PASSWORD=your_sip_password
VOBIZ_OUTBOUND_NUMBER=+918071387318
DEFAULT_TRANSFER_NUMBER=+918071387318

# Optional Configuration
TTS_PROVIDER=deepgram
DEEPGRAM_TTS_MODEL=aura-asteria-en
